<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

    </data>



        <RelativeLayout
            android:id="@+id/customHeightView"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_22sdp">

            <TextView
                android:id="@+id/tvstartTime"
                android:layout_width="@dimen/_18sdp"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:gravity="right"
                android:fontFamily="@font/open_sans_semibold"
                android:textAllCaps="true"
                android:text="3 PM"
                android:textStyle="bold"
                android:textColor="#99ADBB"
                android:textSize="@dimen/_6sdp" />

            <TextView
                android:id="@+id/tvEndTime"
                android:layout_width="@dimen/_12sdp"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:layout_alignParentStart="true"
                android:layout_alignParentBottom="true"
                android:fontFamily="@font/open_sans_bold"
                android:text="4 PM"
                android:textSize="@dimen/_3sdp" />

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1sdp"
                android:visibility="gone"
                android:background="#CCD6DD" />

            <View
                android:id="@+id/tvview7"
                android:layout_marginTop="@dimen/_4sdp"
                android:layout_width="@dimen/_2sdp"
                android:layout_height="match_parent"
                android:layout_marginStart="2dp"
                android:layout_toEndOf="@id/tvstartTime"
                android:background="#F3F3F3" />


            <LinearLayout
                android:id="@+id/timeTableLayout"
                android:layout_marginTop="@dimen/_4sdp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_toEndOf="@id/tvview7"
                android:background="#28cd9c"
                android:orientation="horizontal"
                android:weightSum="7">

                <TextView
                    android:id="@+id/tvTitle"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="10dp"
                    android:layout_weight="1.25"
                    android:fontFamily="@font/open_sans_semibold"
                    android:text="Live Session"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_6sdp" />

                <TextView
                    android:id="@+id/tvSubject"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="30dp"
                    android:layout_weight="1"
                    android:fontFamily="@font/open_sans"
                    android:text="Maths"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_6sdp" />

                <LinearLayout
                    android:id="@+id/iconView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginHorizontal="@dimen/_10sdp"
                    android:layout_marginStart="70dp"
                    android:layout_weight="2"
                    android:gravity="center"
                    android:orientation="horizontal"
                    android:textSize="@dimen/_12sdp">

                    <ImageView
                        android:id="@+id/ivAssessment"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_16sdp"
                        android:layout_weight="1"
                        android:padding="@dimen/_3sdp"
                        android:src="@drawable/pincel" />

                    <ImageView
                        android:id="@+id/ivPreRead"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_16sdp"
                        android:layout_weight="1"
                        android:padding="@dimen/_3sdp"
                        android:src="@drawable/book" />

                    <ImageView
                        android:id="@+id/ivActivity"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_16sdp"
                        android:layout_weight="1"
                        android:padding="@dimen/_3sdp"
                        android:src="@drawable/laptop" />

                    <ImageView
                        android:id="@+id/ivPresentation"
                        android:layout_width="0dp"
                        android:layout_height="@dimen/_16sdp"
                        android:layout_weight="1"
                        android:padding="@dimen/_3sdp"
                        android:src="@drawable/tv" />

                </LinearLayout>

                <TextView
                    android:id="@+id/studyView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="70dp"
                    android:layout_weight="2"
                    android:singleLine="true"
                    android:fontFamily="@font/open_sans"
                    android:text="Complete Chapter 1 &amp; 2"
                    android:textColor="#1298DC"
                    android:textSize="@dimen/_6sdp"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tvDuriation"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="@dimen/_75sdp"
                    android:layout_weight="0.75"
                    android:fontFamily="@font/open_sans"
                    android:text="60 mins"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_6sdp" />

                <ImageView
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_6sdp"
                    android:layout_gravity="center"
                    android:layout_marginStart="60dp"
                    android:layout_weight=".75" />


                <TextView
                    android:id="@+id/tvViewSummeryView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="40dp"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:layout_weight="1.25"
                    android:fontFamily="@font/open_sans"
                    android:text="View summary"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_6sdp"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tvSetReminderView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="40dp"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:layout_weight="1.25"
                    android:fontFamily="@font/open_sans"
                    android:text="@string/set_reminder"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_6sdp"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tvOnReminderView"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="40dp"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:layout_weight="1.25"
                    android:fontFamily="@font/open_sans"
                    android:text="@string/my_text"
                    android:textColor="@color/white"
                    android:textSize="@dimen/_6sdp"
                    android:visibility="gone" />

                <TextView
                    android:id="@+id/tvJoinNowView"
                    android:layout_width="0dp"
                    android:layout_height="@dimen/_10sdp"
                    android:layout_gravity="center"
                    android:layout_marginStart="40dp"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:layout_weight="1.25"
                    android:background="@drawable/corner_textview"
                    android:fontFamily="@font/semibold"
                    android:gravity="center"
                    android:text="JOIN NOW"
                    android:textColor="@color/science_bg_col"
                    android:textSize="8sp"
                    android:visibility="gone"
                    app:backgroundTint="@color/white" />

                <TextView
                    android:id="@+id/tvEditPlaner"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:layout_marginStart="40dp"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:layout_weight="1.25"
                    android:singleLine="true"
                    android:fontFamily="@font/semibold"
                    android:gravity="center"
                    android:text="@string/edit_plan"
                    android:textColor="@color/science_bg_col"
                    android:textSize="@dimen/_6sdp"
                    android:visibility="gone"
                    app:backgroundTint="@color/white" />

            </LinearLayout>
            <LinearLayout
                android:id="@+id/LunchLayout"
                android:layout_marginTop="@dimen/_4sdp"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_toEndOf="@id/tvview7"
                android:background="#F3F3F3"
                android:gravity="center"
                android:visibility="gone"
                android:orientation="horizontal"
                android:weightSum="7">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="LUNCH"
                    android:letterSpacing="1"
                    android:textSize="17sp"
                    android:gravity="center"
                    android:textColor="#FDD3A6"
                    android:fontFamily="@font/open_sans_bold"
                    android:layout_gravity="center"/>


            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/_1sdp"
                android:visibility="gone"
                android:layout_alignParentBottom="true"
                android:layout_toEndOf="@id/tvview7"
                android:background="#CCD6DD" />
        </RelativeLayout>

</layout>